{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/dashboard.html"
    }
  ],
  "env": {
    "FULL_DOMAIN": "mebeatme.ready2race.run",
    "CORS_ORIGINS": "https://mebeatme.ready2race.run,https://ready2race.run",
    "LOG_LEVEL": "INFO",
    "STRAVA_CLIENT_ID": "157217",
    "STRAVA_CLIENT_SECRET": "3652b26562c819e1a13ebb34e517e707dab939b2",
    "DEPLOYMENT_VERSION": "2.3"
  }
}
